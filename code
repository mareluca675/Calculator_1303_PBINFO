#include <bits/stdc++.h>

using namespace std;


int main()
{
    int a, b;
    char operatie;
    
    cin >> a >> b;
    
    if(a < b)
    {
        int aux = a;
        a = b;
        b = aux;
    }
    
    cin >> operatie;
    
    switch(operatie)
    {
        case 45:
            cout << a - b;
            break;
        case 43:
            cout << a + b;
            break;
        case 47:
            cout << a / b;
            break;
        case 42:
            cout << a * b;
            break;
    }
}
